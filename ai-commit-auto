#!/usr/bin/env python3
"""
AI Company Auto Commit Command
最適なプロトコルを自動選択する統合コミットシステム
"""

import os
import sys
from pathlib import Path

# プロジェクトルートディレクトリ
project_root = Path(__file__).parent

# 環境変数設定
os.environ["PYTHONPATH"] = str(project_root)

# Auto Commit実行
import subprocess
subprocess.run([sys.executable, str(project_root / "commands" / "ai_commit_auto.py")] + sys.argv[1:])