# EITMS Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database Configuration
EITMS_DB_PASSWORD=your_database_password_here
EITMS_VECTOR_DB_PASSWORD=your_vector_db_password_here

# GitHub Integration
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here

# Task Tracker Integration
TASK_TRACKER_API_KEY=your_task_tracker_api_key

# Todo Integration  
TODO_API_KEY=your_todo_api_key

# Slack Integration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Security
EITMS_ENCRYPTION_KEY=your_32_byte_encryption_key_here

# Redis Configuration (optional)
REDIS_URL=redis://localhost:6379

# RabbitMQ Configuration (optional)
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# API Configuration
EITMS_API_SECRET_KEY=your_api_secret_key_here

# Environment
EITMS_ENV=development  # development, staging, production

# Logging
EITMS_LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Feature Flags
EITMS_ENABLE_AI_OPTIMIZATION=true
EITMS_ENABLE_AUTO_SYNC=true
EITMS_ENABLE_MONITORING=true