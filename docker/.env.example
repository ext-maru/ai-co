# Elder Flow Environment Configuration Example
# Copy this file to .env and update with your actual values

# Environment
ELDER_FLOW_ENV=development

# Database
POSTGRES_PASSWORD=your-secure-postgres-password
POSTGRES_USER=elderflow
POSTGRES_DB=elderflow

# Redis
REDIS_PASSWORD=

# Security
JWT_SECRET=your-very-secure-jwt-secret-key-here
ADMIN_PASSWORD=your-secure-admin-password

# API Keys
OPENAI_API_KEY=sk-your-openai-api-key

# Monitoring
GRAFANA_PASSWORD=your-secure-grafana-password

# SMTP (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-specific-password

# Enterprise Authentication (optional)
LDAP_URL=ldap://your-ldap-server
LDAP_BIND_DN=cn=admin,dc=example,dc=com
LDAP_BIND_PASSWORD=your-ldap-password

SAML_IDP_URL=https://your-saml-idp/saml
SAML_IDP_CERT=base64-encoded-certificate

# Performance tuning
MAX_WORKERS=16
BATCH_SIZE=50
CACHE_TTL=3600

# Feature flags
ENABLE_QUANTUM_COMPUTING=false
ENABLE_VR_INTERFACE=false
ENABLE_AGI_FEATURES=false
