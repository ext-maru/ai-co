#!/usr/bin/env python3
"""
AI Company Grand Commit Command
至高の品質を追求する最高位コミットプロトコル
"""

import os
import sys
from pathlib import Path

# プロジェクトルートディレクトリ
project_root = Path(__file__).parent

# 環境変数設定
os.environ["PYTHONPATH"] = str(project_root)

# Grand Protocol実行
import subprocess

subprocess.run(
    [sys.executable, str(project_root / "commands" / "ai_commit_grand.py")]
    + sys.argv[1:]
)
